{
    "#type": "scope",
    "scope": "$.current",
    "value": {
        "#type": "switch",
        "value": "$$.http_request.query.profile",
        "cases": {
            "dc": {
                "#type": "switch",
                "value": "$.@datatype.base",
                "cases": {
                    "object": {
                        "#type": "dublin-core-object"
                    },
                    "media": "dublin core for media not yet available",
                    "publication": {
                        "#type": "dublin-core-publication"
                    },
                    "archive": "dublin core for archives not yet available",
                    "exhibition": "dublin core for exhibition not yet available",
                    "package": "dublin core for packages not yet available"
                },
                "default": "Invalid format for datatype"
            },
            "lido": {
              "#type": "switch",
              "value": "$.@datatype.base",
              "cases": {
                "object": { "#type": "lido-object" }
              },
              "default": "lido format for {$.@datatype.base} not available"
            },
            "skos": {
                "#type": "switch",
                "value": "$.@datatype.base",
                "cases": {
                    "object": "Skos xml for objects not yet available",
                    "media": "Skos xml for media not yet available",
                    "publication": "Skos xml for publications not yet available",
                    "archive": "Skos xml for archives not yet available",
                    "exhibition": "Skos xml for exhibition not yet available",
                    "package": "Skos xml for packages not yet available",
                    "concept": {
                        "#type": "process-skos-xml"
                    }
                },
                "default": "Invalid format for datatype"
            },
            "xml": {
                "#type": "switch",
                "value": "$.@datatype.base",
                "cases": {
                    "object": "Lido xml for objects not yet available",
                    "media": "dublin core for media not yet available",
                    "publication": {
                        "#type": "dublin-core-publication"
                    },
                    "archive": "dublin core for archives not yet available",
                    "exhibition": "dublin core for exhibition not yet available",
                    "package": "dublin core for packages not yet available",
                    "concept": {
                        "#type": "skos-xml"
                    }
                },
                "default": "Invalid format for datatype"
            }
        },
        "default": {
            "#type": "switch",
            "value": "$.@datatype.base",
            "cases": {
                "object": {
                    "#type": "dublin-core-object"
                },
                "media": "dublin core for media not yet available",
                "publication": {
                    "#type": "dublin-core-publication"
                },
                "archive": "dublin core for archives not yet available",
                "exhibition": "dublin core for exhibition not yet available",
                "package": "dublin core for packages not yet available",
                "concept": {
                    "#type": "skos"
                }
            },
            "default": "Invalid format for datatype"
        }
    }
}