{
    "process_linked_art_place": {
        "#type": "merge",
        "values": [
            {
                "#type": "linked_art_context"
            },
            {
                "#type": "linked_art_type"
            },
            {
                "#type": "linked_art_label"
            },
            {
                "#type": "linked_art_identified_by"
            },
            {
                "#type": "linked_art_part_of"
            },
            {
                "#type": "linked_art_place_coordinates"
            }
        ]
    },
    "linked_art_place_coordinates": {
        "#type": "require",
        "require": "$.coordinates[?(@.format=='WKT')]",
        "value": {
            "defined_by": "$.coordinates[?(@.format=='WKT')].value"
        }
    }
}