{
    "process_linked_art_dokuwiki": {
        "#type": "merge",
        "values": [
            {
                "#type": "linked_art_context"
            },
            {
                "#type": "linked_art_type"
            },
            {
                "#type": "linked_art_label"
            },
            {
                "#type": "linked_art_dokuwiki_classified_as"
            },
            {
                "#type": "linked_art_dokuwiki_identified_by"
            },
            {
                "#type": "linked_art_dokuwiki_referred_to_by"
            },
            {
                "#type": "linked_art_dokuwiki_about_agent"
            },
            {
                "#type": "linked_art_dokuwiki_content"
            }
        ]
    },
    "linked_art_dokuwiki_classified_as": {
        "classified_as": {
            "#type": "as_list",
            "values": [
                {
                    "#type": "linked_art_dokuwiki_classified_as_wiki"
                },
                {
                    "#type": "linked_art_dokuwiki_classified_as_full_text"
                }
            ]
        }
    },
    "linked_art_dokuwiki_classified_as_wiki": {
        "#type": "merge",
        "values": [
            {
                "id": "http://vocab.getty.edu/aat/300266678",
                "type": "Type",
                "_label": "wikis",
                "classified_as": [
                    {
                        "id": "http://vocab.getty.edu/aat/300435443",
                        "type": "Type",
                        "_label": "Type of Work"
                    }
                ]
            }
        ]
    },
    "linked_art_dokuwiki_classified_as_full_text": {
        "#type": "chain",
        "chain": [
            {
                "#type": "regex_replace",
                "value": "$.summary.title",
                "pattern": {
                    "#type": "literal",
                    "value": "^(paintings:|projects:).*"
                },
                "replacement": {
                    "#type": "literal",
                    "value": "$1"
                }
            },
            {
                "#type": "switch",
                "value": "$",
                "cases": {
                    "paintings:": {},
                    "projects:": {}
                },
                "default": {
                    "id": "http://vocab.getty.edu/aat/300418050",
                    "type": "Type",
                    "_label": "full texts"
                }
            }
        ]
    },
    "linked_art_dokuwiki_identified_by": {
        "identified_by": {
            "#type": "merge",
            "values": [
                {
                    "#type": "linked_art_dokuwiki_identified_by_title"
                },
                {
                    "#type": "linked_art_identified_by_identifier"
                }
            ]
        }
    },
    "linked_art_dokuwiki_referred_to_by": {
        "referred_to_by": {
            "#type": "merge",
            "values": [
                {
                    "#type": "linked_art_referred_to_by_rights"
                }
            ]
        }
    },
    "linked_art_dokuwiki_about_agent": {
        "about": {
            "#type": "for_each",
            "values": "$.agent",
            "spec": {
                "#type": "link_to_record"
            }
        }
    },
    "linked_art_dokuwiki_produced_by": {
        "produced_by": [
            {
                "type": "Production",
                "carried_out_by": [
                    {
                        "#type": "merge",
                        "values": [
                            {
                                "id": {
                                    "#type": "chain",
                                    "chain": [
                                        "$.data.author",
                                        {
                                            "#type": "linked_art_dokuwiki_specious_person_url"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "Person",
                                "_label": "Author: {$.data.author.value}"
                            }
                        ]
                    },
                    {
                        "#type": "merge",
                        "values": [
                            {
                                "id": {
                                    "#type": "chain",
                                    "chain": [
                                        "$.data.editor",
                                        {
                                            "#type": "linked_art_dokuwiki_specious_person_url"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "Person",
                                "_label": "Editor: {$.data.editor.value}"
                            }
                        ]
                    },
                    {
                        "#type": "merge",
                        "values": [
                            {
                                "id": {
                                    "#type": "chain",
                                    "chain": [
                                        "$.data.curator",
                                        {
                                            "#type": "linked_art_dokuwiki_specious_person_url"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "Person",
                                "_label": "Curator: {$.data.curator.value}"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "linked_art_dokuwiki_identified_by_title": {
        "type": "Name",
        "classified_as": [
            {
                "#type": "linked_art_name_type"
            }
        ],
        "content": "$.data.page_title.value"
    },
    "linked_art_dokuwiki_specious_person_url": {
        "#type": "string_join",
        "values": [
            "{%.linked_art_base_url}linked-data-placeholder/person/",
            {
                "#type": "string_to_case",
                "case": "upper_camel",
                "value": "$.value"
            }
        ]
    },
    "linked_art_dokuwiki_content": {
        "#type": "chain",
        "chain": [
            {
                "#type": "regex_replace",
                "value": "$.summary.title",
                "pattern": {
                    "#type": "literal",
                    "value": "^(paintings:|projects:lbso:|projects:dbic:).*"
                },
                "replacement": {
                    "#type": "literal",
                    "value": "$1"
                }
            },
            {
                "#type": "switch",
                "value": "$",
                "cases": {
                    "paintings:": {
                        "#type": "linked_art_dokuwiki_painting_content"
                    },
                    "projects:lbso:": {
                        "#type": "linked_art_dokuwiki_project_content"
                    },
                    "projects:dbic:": {
                        "#type": "linked_art_dokuwiki_project_content"
                    }
                },
                "default": {
                    "content": "$$.*.text.all.formatted"
                }
            }
        ]
    },
    "linked_art_dokuwiki_painting_content": {
        "part": [
            {
                "type": "LinguisticObject",
                "_label": "short text",
                "classified_as": [
                    {
                        "id": "http://vocab.getty.edu/aat/300418049",
                        "type": "Type",
                        "_label": "brief texts"
                    }
                ],
                "content": "$$.*.text.short.formatted"
            },
            {
                "type": "LinguisticObject",
                "_label": "long text",
                "classified_as": [
                    {
                        "id": "http://vocab.getty.edu/aat/300418050",
                        "type": "Type",
                        "_label": "full texts"
                    }
                ],
                "content": "$$.*.text.long.formatted"
            }
        ]
    },
    "linked_art_dokuwiki_project_content": {
        "part": {
            "#type": "flatten",
            "values": [
                {
                    "#type": "lbso_abolition"
                },
                {
                    "#type": "lbso_slavery"
                },
                {
                    "#type": "lbso_painting"
                },
                {
                    "#type": "lbso_biography"
                }
            ]
        }
    },
    "lbso_abolition": {
        "type": "LinguisticObject",
        "_label": "abolition",
        "classified_as": [
            {
                "id": "http://vocab.getty.edu/aat/300418050",
                "type": "Type",
                "_label": "full texts"
            }
        ],
        "content": "$$.*.abolition.formatted"
    },
    "lbso_slavery": {
        "type": "LinguisticObject",
        "_label": "slavery",
        "classified_as": [
            {
                "id": "http://vocab.getty.edu/aat/300418050",
                "type": "Type",
                "_label": "full texts"
            }
        ],
        "content": "$$.*.slavery.formatted"
    },
    "lbso_painting": {
        "type": "LinguisticObject",
        "_label": "painting",
        "classified_as": [
            {
                "id": "http://vocab.getty.edu/aat/300418050",
                "type": "Type",
                "_label": "full texts"
            }
        ],
        "content": "$$.*.painting.formatted"
    },
    "lbso_biography": {
        "type": "LinguisticObject",
        "_label": "biography",
        "classified_as": [
            {
                "id": "http://vocab.getty.edu/aat/300418050",
                "type": "Type",
                "_label": "full texts"
            }
        ],
        "content": "$$.*.biography.formatted"
    }
}