{
    "process_dublin_core_exhibition": {
        "@xmlformat": "jml",
        "oai_dc:dc": {
            "@attributes": {
                "xmlns:dc": "http://purl.org/dc/elements/1.1/",
                "xmlns:dcterms": "http://purl.org/dc/terms/",
                "xmlns:oai_dc": "http://www.openarchives.org/OAI/2.0/oai_dc/",
                "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
                "xsi:schemaLocation": "http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd http://purl.org/dc/terms/ https://www.dublincore.org/schemas/xmls/qdc/dcterms.xsd"
            },
            "@children": {
                "#type": "flatten",
                "values": [
                    {
                        "#type": "dc_identifier_exhibition"
                    },
                    {
                        "#type": "dc_title_exhibition"
                    },
                    {
                        "#type": "dcterms_alternative_exhibition"
                    },
                    {
                        "#type": "dc_date"
                    },
                    {
                        "#type": "dc_creator_exhibition"
                    },
                    {
                        "#type": "dc_contributor_exhibition"
                    },
                    {
                        "#type": "dc_publisher_exhibition"
                    },
                    {
                        "#type": "dcterms_temporal_exhibition"
                    },
                    {
                        "#type": "dcterms_hasPart_exhibition"
                    },
                    {
                        "#type": "dc_description"
                    },
                    {
                        "#type": "dc_type_exhibition"
                    },
                    {
                        "#type": "dcterms_isReferencedBy_exhibition"
                    },
                    {
                        "#type": "dc_rights"
                    }
                ]
            }
        }
    },
    "dcterms_alternative_exhibition": {
        "#type": "for_each",
        "values": "$.venue",
        "spec": {
            "#type": "chain",
            "chain": [
                "$.@link.title[0].value",
                {
                    "dcterms:alternative": {
                        "#type": "escape_xml"
                    }
                }
            ]
        }
    },
    "dc_contributor_exhibition": {
        "#type": "flatten",
        "values": [
            {
                "#type": "for_each",
                "values": "$.agent[?(@.@link.role[0].value in ['NG Curator', 'Curator'])]",
                "spec": {
                    "dc:contributor": "$.summary.title"
                }
            }
        ]
    },
    "dc_creator_exhibition": {
        "#type": "for_each",
        "values": "$.venue",
        "spec": {
            "#type": "flatten",
            "values": [
                {
                    "dc:creator": {
                        "#type": "generic_xml_url"
                    }
                },
                {
                    "dc:creator": {
                        "#type": "escaped_title"
                    }
                }
            ]
        }
    },
    "dcterms_hasPart_exhibition": {
        "#type": "flatten",
        "values": [
            {
                "#type": "for_each",
                "values": "$.venue",
                "spec": {
                    "dcterms:hasPart": {
                        "#type": "string_join",
                        "delimiter": ", ",
                        "values": [
                            "$.@link.title[0].value",
                            "$.summary.title",
                            "$.@link.date[0].value"
                        ]
                    }
                }
            }
        ]
    },
    "dc_identifier_exhibition": {
        "dc:identifier": {
            "#type": "generic_xml_url"
        }
    },
    "dcterms_isReferencedBy_exhibition": {
        "#type": "flatten",
        "values": [
            {
                "#type": "for_each",
                "values": "$.bibliography",
                "spec": {
                    "#type": "flatten",
                    "values": [
                        {
                            "dcterms:isReferencedBy": {
                                "#type": "generic_xml_url"
                            }
                        },
                        {
                            "dcterms:isReferencedBy": {
                                "#type": "escaped_title"
                            }
                        }
                    ]
                }
            }
        ]
    },
    "dc_publisher_exhibition": {
        "#type": "for_each",
        "values": "$.venue",
        "spec": {
            "#type": "flatten",
            "values": [
                {
                    "dc:publisher": {
                        "#type": "generic_xml_url"
                    }
                },
                {
                    "dc:publisher": {
                        "#type": "escaped_title"
                    }
                }
            ]
        }
    },
    "dcterms_temporal_exhibition": {
        "#type": "for_each",
        "values": "$.date",
        "spec": {
            "dcterms:temporal": "$.value"
        }
    },
    "dc_title_exhibition": {
        "#type": "for_each",
        "values": "$.venue",
        "spec": {
            "#type": "flatten",
            "values": [
                {
                    "#type": "for_each",
                    "values": "$.@link.title",
                    "spec": {
                        "#type": "chain",
                        "chain": [
                            "$.value",
                            {
                                "dc:title": {
                                    "#type": "escape_xml"
                                }
                            }
                        ]
                    }
                }
            ]
        }
    },
    "dc_type_exhibition": {
        "#type": "flatten",
        "values": [
            {
                "dc:type": "http://purl.org/dc/dcmitype/Event"
            },
            {
                "dc:type": "Event"
            }
        ]
    }
}